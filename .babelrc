{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-inline-import",
      {
        "extensions": [".css"]
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-styled-components"
  ]
}
